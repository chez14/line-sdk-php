{
    "name": "chez14/line-sdk",
    "description": "Line SDK Client for PHP. Used for simple Login API or Social API.",
    "keywords": ["LINE", "LINE SDK","SDK", "PHP", "Login API", "LINE Login"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christianto",
            "email": "chris@christianto.net"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3",
        "lcobucci/jwt": "^3.2",
        "ircmaxell/random-lib": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "LINE\\": "src/"
        }
    },
    "scripts": {
        "test-dry" : "phpunit --testdox",
        "test":"composer require phpunit/phpunit && phpunit --testdox"
    }
}
